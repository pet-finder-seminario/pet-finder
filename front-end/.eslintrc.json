{
    "env": {
        "browser": true,
        "es6": true
    },
    "extends": [
        "airbnb"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "react/jsx-props-no-spreading": "off",
        "react/forbid-prop-types": "off",
        "react/require-default-props": "off",
        "import/prefer-default-export": "off",
        "arrow-parens": "off",
        "react/static-property-placement": "off"
    },
    "parser": "babel-eslint"
}